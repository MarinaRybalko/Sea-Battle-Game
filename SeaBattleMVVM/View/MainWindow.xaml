<Window x:Class="SeaBattleMVVM.MainWindow"
        x:Name="SeaBattle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" 
        xmlns:l="clr-namespace:LocalizatorHelper;assembly=LocalizatorHelper"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="sea Battle" Height="560" Width="900" Icon="pack://siteoforigin:,,,/Resources/MainIcon.ico">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <l:LocalisationHelper x:Key="Helper"></l:LocalisationHelper>
    </Window.Resources>
    <i:Interaction.Triggers>

        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Path=GreetingCommand}"></i:InvokeCommandAction>
        </i:EventTrigger>

    </i:Interaction.Triggers>
    <Grid Background="LightSkyBlue" >

        <ToolBar x:Name="MainMenuToolBar" HorizontalAlignment="Left" VerticalAlignment="Top" Height="25" Width="2000" Background="SteelBlue">
            <Button x:Name="ExitButton" Content="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.Exit_Btn]}" Width="50" Height="20" Command="{Binding ExitCommand, Mode=OneWay}" 
                    CommandParameter="{Binding ElementName=SeaBattle}"/>
            <Button x:Name="InformationButton" Content="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.Inform_Btn]}" Width="90" Height="20" Command="{Binding Path = InformationCommand}"/>
            <Button x:Name="TopPlayersButton" Content="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.TopPlayers_Btn]}" Width="109" Height="22" Command="{Binding Path=TopPlayersCommand}" Margin="0,-1,0,-1.8"/>
            <Menu x:Name="Menu" Height="21" VerticalAlignment="Top" Width="145" Background="SteelBlue" Margin="0,0,0,-1.8">
                <MenuItem Header="{Binding [MainWindowRes.Language_Btn], Source={StaticResource Helper}}" Height="22" Margin="0">
                    <MenuItem Header="Русский" Command="{Binding RussianCommand}"/>
                    <MenuItem Header="English" Command="{Binding EnglishCommand}"/>
                </MenuItem>
            </Menu>
        </ToolBar>
        <Canvas HorizontalAlignment="Left" Height="35" Margin="3,25,0,0" VerticalAlignment="Top" Width="174" Background="SteelBlue" Visibility="{Binding Path= IsButtonsStartVisible, Converter={StaticResource BooleanToVisibility}}"  >
            <TextBlock Name="ItemsControlGameMode" Text="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.GameModes_Lbl]}"  Margin="0,0,0,0" Canvas.Left="23" Canvas.Top="0" Padding="0,0,0,5"/>
            <RadioButton Name="RadioButtonNormal" Content="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.Normal_Lbl]}"  IsChecked="{Binding Path=IsGameModeNormal}" Margin="0,15,5,0" GroupName="Modes"  />
            <RadioButton Name="RadioButtonEasy" Content="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.Easy_Lbl]}" Margin="70,15,5,0" GroupName="Modes"/>
        </Canvas>
        <Canvas Name="CanvasFields">
            <TextBlock x:Name="BlockPlayerStatistics" FontStyle="Italic" Canvas.Left="30" Canvas.Top="69" >
                    <TextBlock x:Name="BlockName" Text="{Binding [MainWindowRes.YourField_Lbl], Source={StaticResource Helper}}" FontStyle="Italic" FontFamily="Monotype Corsiva" Width="190" Height="30" TextAlignment="Right" FontSize="24"/>
                    <LineBreak></LineBreak>
                    <TextBlock Name="BlockShot" Text="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.ShotAmount_Lbl]}" Padding="20,0,0,0" FontSize="20" FontStyle="Italic" FontFamily="Monotype Corsiva"/>
                    <Label Name="LabelShotYourField" Content="{Binding Path=ShotAmountLeft}" FontSize="18" Padding="0,-15,0,0" FontStyle="Italic" FontFamily="Monotype Corsiva" Height="15" Width="25"/>
                    <TextBlock Name="BlockShip" Text="{Binding Source={StaticResource Helper}, Path=[MainWindowRes.ShipAmount_Lbl]}"  Padding="20,0,0,0" FontSize="20" FontStyle="Italic" FontFamily="Monotype Corsiva"/>  
                    <Label Name="LabelShipYourField" Content="{Binding Path=ShipAmountLeft}" FontSize="18" Padding="0,-15,0,0" FontStyle="Italic" FontFamily="Monotype Corsiva" Height="15" Width="25"/>
            </TextBlock>
            <UniformGrid Name="UniformGridLeftField"  Width="330" Height="330" Background="Gray" HorizontalAlignment="Left" Margin="30,130,0,0">
